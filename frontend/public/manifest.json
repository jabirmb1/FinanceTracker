{
  "manifest_version": 3,
  "name": "Finance Tracker",
  "version": "1.0.0",
  "description": "Base Chrome Extension using React + TypeScript",
  "action": {
    "default_popup": "index.html",
    "default_title": "Finance Tracker",
    "default_icon": {
      "16": "icons16.png",
      "48": "icon48.png",
      "128": "logo512.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"]
}
